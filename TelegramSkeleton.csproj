<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>
        <AssemblyName>Team23.TelegramSkeleton</AssemblyName>
        <RootNamespace>Team23.TelegramSkeleton</RootNamespace>
        <PackageId>Team23.TelegramSkeleton</PackageId>
        <Authors>Team23</Authors>
        <LangVersion>latest</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <DefineConstants>JETBRAINS_ANNOTATIONS</DefineConstants>
        <Version>1.0.28</Version>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Autofac" Version="4.9.4" />
      <PackageReference Include="JetBrains.Annotations" Version="2019.1.3" />
      <PackageReference Include="Microsoft.ApplicationInsights.AspNetCore" Version="2.8.0" />
      <PackageReference Include="Microsoft.AspNetCore.App" />
      <PackageReference Include="NodaTime" Version="2.4.7" />
      <PackageReference Include="NodaTime.Testing" Version="2.4.7" />
      <PackageReference Include="Telegram.Bot" Version="15.0.0" />
    </ItemGroup>

</Project>
